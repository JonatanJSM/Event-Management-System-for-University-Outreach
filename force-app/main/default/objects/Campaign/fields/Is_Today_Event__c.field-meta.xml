<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Is_Today_Event__c</fullName>
    <description>Indicates whether the scheduled event coincides with the current date.</description>
    <formula>TODAY() = DATEVALUE(Event_Date__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is Today Event</label>
    <type>Checkbox</type>
</CustomField>
